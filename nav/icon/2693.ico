<script>
  $('.footer-leaveMessage .top').click(function(){
    var par = $(this).parent()
    if(par.hasClass('curr')){
      par.removeClass('curr')
      $(this).find('a').html('&#xe65d;')
    }else{
      par.addClass('curr')
      $(this).find('a').html('&#xeca0;')
    }
  })
    function refreshVcodeForLogin() {

        var src = '/common/GenerateCheckCode.aspx?t=sign&temp=' + Math.random();
        $(".VcodeImg").attr("src", src);
    }
    function submits(o) {
        var obj = $(o);
        var findobj = obj.parents(".formCon");
        var name = findobj.find(".name").val();
        var tel = findobj.find(".tel").val();
        var wxqq = findobj.find(".wxqq").val();
        var types = findobj.find(".types").val();
        var content = findobj.find(".content").val();
        var codes = findobj.find(".codes").val();
        var params = {
            "module": "addbutton",
            "a_unme": name,
            "a_gender": "",
            "a_businesstype": "",
            "a_province": "",
            "a_city": "",
            "a_phone": tel,
            "a_qq": wxqq,
            "a_memo": content,
            "a_eml": "",
            "code": codes,
            "ywulx": types
        };
        $("#submits").removeAttr("onclick");
        var from_tip = $("#from_tip");
        from_tip.find("img").show();
        $.post("/Ajax/leavemsg.ashx", params, function (msg) {
            $("#submits").attr("onclick", "submits(this);");
            from_tip.find("img").hide();
            if (msg.code == "10000") {
                alert("感谢提交，我们会尽快联系您！");
                 findobj.find(".name").val("");
         findobj.find(".tel").val("");
             findobj.find(".wxqq").val("");
           findobj.find(".types").val("");
         findobj.find(".content").val("");
           findobj.find(".codes").val("");
            } else {
                alert(msg.message);
            }
            refreshVcodeForLogin();
        }, "json");
       
    }
</script>

